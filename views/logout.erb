<%= erb :header %>


<!-- WEBSITE ARTICLES -->
<div id="maincontent">
  <article id="form">
    <h1>You have been successfully logged out!</h1>
    <form action="login_validate" method="post" >
      <table>
        <tr>
          <td>Username: </td>
          <td><input type="text" size="20" name="username" /></td>
        </tr>

        <tr>
          <td>Password: </td>
          <td><input type="password" size="20" name="password" /></td>
        </tr>

        <tr>
          <td><input id="sub_button" type="submit" value="Submit" /></td>
        </tr>
      </table>
    </form>
  </article>
</div>


<%= erb :footer %>